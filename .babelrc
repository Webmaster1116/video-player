{
  "presets": ["react", "es2015", "stage-0"],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "plugins": [
        "add-module-exports",
        "transform-es3-member-expression-literals",
        "transform-es3-property-literals"
      ]
    }
  }
}
